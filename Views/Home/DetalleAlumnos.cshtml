@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <table>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Legajo</th>
            <th>Curso</th>
            <th>Curso</th>
        </tr>

        @foreach (Alumnos alumno in ViewBag.DetalleAlumno){
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.Apellido</td>
                <td>@alumno.Legajo</td>
                <td>@alumno.Curso</td>
            </tr>
        }
        @foreach (Notas nota in ViewBag.NotasAlumno){
            <tr>
                <td>@nota.Nota</td>
            </tr>
        }

    </table>
</div>
